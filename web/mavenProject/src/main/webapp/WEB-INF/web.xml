<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
  <servlet>
	<servlet-name>Home</servlet-name>
	<display-name>Home</display-name>
	<description></description>
	<servlet-class>com.Hello</servlet-class>
</servlet>
  <servlet>
  	<servlet-name>AddInformationServlet</servlet-name>
  	<display-name>AddInformationServlet</display-name>
  	<description></description>
  	<servlet-class>com.AddInformationServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UpdateInformationServlet</servlet-name>
  	<display-name>UpdateInformationServlet</display-name>
  	<description></description>
  	<servlet-class>com.UpdateInformationServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ViewInformationServlet</servlet-name>
  	<display-name>ViewInformationServlet</display-name>
  	<description></description>
  	<servlet-class>com.ViewInformationServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DeleteUserServlet</servlet-name>
  	<display-name>DeleteUserServlet</display-name>
  	<description></description>
  	<servlet-class>com.DeleteUserServlet</servlet-class>
  </servlet>

  <servlet-mapping>
	<servlet-name>Home</servlet-name>
	<url-pattern>/home</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AddInformationServlet</servlet-name>
  	<url-pattern>/addinfo</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UpdateInformationServlet</servlet-name>
  	<url-pattern>/updateinfo</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ViewInformationServlet</servlet-name>
  	<url-pattern>/viewinfo</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DeleteUserServlet</servlet-name>
  	<url-pattern>/deleteuser</url-pattern>
  </servlet-mapping>
</web-app>
